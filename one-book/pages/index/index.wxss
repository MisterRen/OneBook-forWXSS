/**index.wxss**/
.container1{
  width:99%;
  height:auto;
  margin: 0 auto;
}
.liImg{
  float: left;
  margin:5px;
  width:30%;
  height:auto;
  padding:5px;
  box-sizing: border-box;
  font-size:12px;
}
.label{
  overflow: hidden;
  width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: center;
}
.liImg>label:nth-child(1)>image{
  margin: 0 auto;
  display: block;
  width:100%;
  height: 120px;
}
page {  
  display: block;  
  min-height: 100%;  
  background-color: #F8F8F8;  
}